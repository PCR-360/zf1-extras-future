{
    "name": "pcr/zf1-extras-future",
    "description": "Zend Framework 1 Extras The aim is to keep ZF1 Extras working with the latest PHP versions",
    "type": "library",
    "version": "24.2.1",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "url": "https://github.com/PCR-360/zf1-future",
            "type": "git"
        },
        {
            "type": "composer",
            "url": "https://packages.pcr.com"
        }
    ],
    "require": {
        "php": ">=5.2.11",
        "pcr/zf1-future": "^24.2.2"
    },
    "autoload": {
        "psr-0": {
            "ZendX_": "library/"
        }
    },
    "include-path": [
        "library/"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.12.x-dev"
        }
    },
    "replace": {
        "zendframework/zf1-extras": "1.*",
        "shardj/zf1-extras-future": "*"
    }
}
